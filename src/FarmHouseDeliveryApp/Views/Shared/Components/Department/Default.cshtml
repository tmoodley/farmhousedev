@model IEnumerable<FarmHouseDeliveryApp.Models.Department>

<div class="collapse navbar-collapse" id="bs-megadropdown-tabs">
    <ul class="nav navbar-nav nav_1">
        @foreach (var item in Model)
            {
            <li>
                <a href="/d/@Html.DisplayFor(modelItem => item.Name)/" title="@Html.DisplayFor(modelItem => item.Description)">@Html.DisplayFor(modelItem => item.Description)</a>
            </li>
        }
    </ul>
</div> 